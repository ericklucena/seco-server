<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ManholeInsert</servlet-name>
		<servlet-class>co.codehaven.seco.servlets.ManholeInsertServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ManholeInsert</servlet-name>
		<url-pattern>/manhole/insert</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ManholeMaintenance</servlet-name>
		<servlet-class>co.codehaven.seco.servlets.ManholeMaintenanceServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ManholeMaintenance</servlet-name>
		<url-pattern>/manhole/maintenance</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ManholeSync</servlet-name>
		<servlet-class>co.codehaven.seco.servlets.ManholeSyncServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ManholeSync</servlet-name>
		<url-pattern>/manhole/sync</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ManholeGet</servlet-name>
		<servlet-class>co.codehaven.seco.servlets.ManholeGetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ManholeGet</servlet-name>
		<url-pattern>/manhole/get</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
